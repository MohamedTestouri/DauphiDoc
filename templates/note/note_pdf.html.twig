    <h1>Note index</h1>
    <table class="table">
        <thead>
        <tr>
            <th>Idnote</th>
            <th>Nomtest</th>
            <th>Nomeleve</th>
            <th>Note</th>
            <th>Resultat</th>
        </tr>
        </thead>
        <tbody>
        {% for note in notes %}
            <tr>
                <td>{{ note.idnote }}</td>
                <td>{{ note.nomtest }}</td>
                <td>{{ note.nomeleve }}</td>
                <td>{{ note.note }}</td>
                <td>{{ note.resultat }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
